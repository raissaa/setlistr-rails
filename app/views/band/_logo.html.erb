<% if band.logo.url != nil %>
  <% if size == '200' %>
    <%= image_tag band.logo.small.url %>
  <% elsif size == '100' %>
    <% puts 'piaa' %>
    <%= image_tag band.logo.smaller.url %>
  <% else %>
    <% puts size %>
    <%= image_tag band.logo.url %>
  <% end %>
<% else %>
  <% if size == '200' %>
    <%= image_tag 'band_logo_placeholder200.png' %>
  <% else %>
    <%= image_tag 'band_logo_placeholder100.png' %>
  <% end %>
<% end %>