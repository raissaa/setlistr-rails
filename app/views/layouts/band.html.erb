<% content_for :content do %>
<div class="container">
  <div class="row">
      <div class="col-md-2 sidebar">
          <h4 class="text-center"><%= link_to @band.name, @band %></h4>
          <ul class="nav nav-sidebar">
              <li id="nav_setlist"><%= link_to "Setlist", band_setlist_path(@band) %></li>
          </ul>
          <ul class="nav nav-sidebar">
              <li id="navUsuarios"></li>
          </ul>
          <ul class="nav nav-sidebar">
              <li id="navSobre"></li>
          </ul>
      </div>
      
      <div class="col-md-9 col-md-offset-1 main">
          <%= yield %>
      </div>
  </div>
</div>
<% end %>
<%= render template: "layouts/application" %>